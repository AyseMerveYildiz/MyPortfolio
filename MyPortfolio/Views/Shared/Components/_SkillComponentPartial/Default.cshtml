@model List<MyPortfolio.DAL.Entities.Skill>
<div class="col-six tab-full right">
    <h3>Programlama dilleri & Teknolojileri</h3>

    <ul class="skill-bars">
        @foreach(var item in Model)
        {
            <li>
                <div class="progress "><span>@item.Value</span></div>
                <strong>@item.Title</strong>
            </li>
        }
       
    </ul>
</div>